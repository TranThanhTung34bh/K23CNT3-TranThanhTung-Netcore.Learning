@model TttLap03.Models.Account

<div>
    <img src="~/images/@Model.Image" width="200" />
    <h2>Your Profile</h2>
    <p><strong>Full Name:</strong> @Model.FullName</p>
    <p><strong>Email:</strong> @Model.Email</p>
    <p><strong>Phone:</strong> @Model.Phone</p>
    <p><strong>Address:</strong> @Model.Address</p>
    <p><strong>Gender:</strong> @Model.Gender</p>
    <p><strong>Birthday:</strong> @Model.Birthday</p>
    <p><strong>Bio:</strong> @Model.Bio</p>
    <a href="/TttAccount" class="btn btn-danger">Back to list</a>
</div>
